{
  "name": "react-start-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-actions": "5.3.0-beta.21",
    "@storybook/addon-links": "5.3.0-beta.21",
    "@storybook/addons": "5.3.0-beta.21",
    "@storybook/preset-create-react-app": "1.3.2",
    "@storybook/react": "5.3.0-beta.21",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.16",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "cross-env": "6.0.3",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "source-map-explorer": "2.1.2",
    "stylelint": "12.0.0",
    "stylelint-config-recommended-scss": "4.1.0",
    "stylelint-scss": "3.13.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.3"
  },
  "scripts": {
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "stylelint": "stylelint src/**/*.scss",
    "stylelintAndFix": "stylelint src/**/*.scss --fix",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslintAndFix": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
