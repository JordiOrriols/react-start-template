{
  "name": "react-start-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storyshots-puppeteer": "^5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "5.11.1",
    "@testing-library/react": "10.4.7",
    "@testing-library/user-event": "12.0.11",
    "@types/jest": "26.0.5",
    "@types/node": "12.12.16",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "cross-env": "7.0.2",
    "husky": "^4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "puppeteer": "^5.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "2.4.2",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0",
    "typescript": "3.9.7",
    "webpack": "4.42.0"
  },
  "scripts": {
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "stylelint": "stylelint src/**/*.css",
    "stylelintAndFix": "stylelint src/**/*.css --fix",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslintAndFix": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}